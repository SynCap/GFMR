{
   "content_scripts": [ {
      "js": [ 
         "js/markdown-it.min.js" ,
         "js/markdown-it-deflist.min.js",
         "js/markdown-it-emoji-light.min.js",
         "js/hl.js" ,
         "js/hl-ln.js" ,
         "js/init.js" 
      ],
      "matches": [ 
         "http://*/*.MD", "http://*/*.md*", "*://*/*.markdown*", "http://*/*.text", 
         // "https://*/*.MD", "https://*/*.md*", "*://*/*.markdown*", "https://*/*.text", 
         "file://*/*.MD", "file://*/*.md*", "*://*/*.markdown*", "file://*/*.text"
      ]
   } ],
   "web_accessible_resources": [ "css/gfmr.css", "css/*.css", "img/*.png", "img/*.ico" ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Converts and previews markdown files (.md, .markdown) as HTML right inside Chrome.",
   "icons": {
      "128": "img/icon.png",
      "48":  "img/icon.png",
      "16":  "img/icon.png"
   },
   "name": "Github Flavored Markdown Renderer",
   "permissions": [ "tabs", "\u003Call_urls\u003E" ],
   "manifest_version": 2,
   "version": "0.2.1"
}
