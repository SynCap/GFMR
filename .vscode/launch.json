{
	"version": "0.2.0",
	"configurations": [{
		"name": "Запустить",
		"type": "node",
		"request": "launch",
		"program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
		"stopOnEntry": false,
		"args": [
			"css",
			"--dev-mode" 
		],
		"cwd": "${workspaceRoot}",
		"preLaunchTask": null,
		"runtimeExecutable": null,
